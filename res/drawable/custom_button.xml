<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
     <item android:state_pressed="true" >
         <shape android:shape="rectangle"  >
             <corners android:radius="3dip" />
             <stroke android:width="1dip" android:color="#2E3A6B" />
             <gradient  android:angle="-90"  android:startColor="#101010" android:endColor="#000000"  />            
         </shape>
     </item>
    <item android:state_focused="true">
         <shape android:shape="rectangle"  >
             <corners android:radius="3dip" />
             <stroke android:width="1dip" android:color="#46579C" />
             <solid  android:color="#92806c"/>       
         </shape>
     </item>  
    <item >
        <shape android:shape="rectangle"  >
             <corners android:radius="3dip" />
             <stroke android:width="1dip" android:color="#46579C" />
             <gradient  android:angle="-90"  android:startColor="#202020" android:endColor="#1A1A1A" />            
         </shape>
     </item>
</selector>